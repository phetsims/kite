{
  "name": "kite",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/kite.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3",
    "pegjs": "~0.7.0"
  },
  "phet": {
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "generatedUnitTests": true,
    "requirejsNamespace": "KITE",
    "buildStandalone": true,
    "requiresLodash": true,
    "assignGlobals": {
      "kite": "KITE/main",
      "dot": "DOT/main",
      "axon": "AXON/main",
      "phetCore": "PHET_CORE/main"
    },
    "preload": [
      "../sherpa/lib/flatqueue-1.2.1.js"
    ],
    "supportsOutputJS": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "rules": {
      "todo-should-have-issue": "off"
    }
  }
}